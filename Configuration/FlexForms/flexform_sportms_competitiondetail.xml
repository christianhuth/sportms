<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <base>EXT:sportms/Configuration/FlexForms/flexform_base.xml</base>
        <competition>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:sportms/Resources/Private/Language/locallang.xlf:tx_sportms_domain_model_competition
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.competition.uid>
                        <TCEforms>
                            <label>Zeige folgenden Wettbewerb</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_sportms_domain_model_competition</allowed>
                                <size>1</size>
                                <maxItems>1</maxItems>
                                <minItems>0</minItems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.competition.uid>
                    <settings.competition.navigation.enabled>
                        <TCEforms>
                            <label>Wettbewerbs-Navigation anzeigen?</label>
                            <config>
                                <default>1</default>
                                <renderType>checkboxToggle</renderType>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.competition.navigation.enabled>
                    <settings.competitionSeasonGameday.selectbox.enabled>
                        <TCEforms>
                            <label>Selectbox f√ºr Spieltage anzeigen?</label>
                            <config>
                                <default>1</default>
                                <renderType>checkboxToggle</renderType>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.competitionSeasonGameday.selectbox.enabled>
                </el>
            </ROOT>
        </competition>
        <season>EXT:sportms/Configuration/FlexForms/flexform_season.xml</season>
    </sheets>
</T3DataStructure>