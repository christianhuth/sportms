<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<ROOT>
		<TCEforms>
			<sheetTitle>Mannschaftsinstanz</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<settings.teamseason.uid>
				<TCEforms>
					<label>Zeige folgende Mannschaftsinstanz</label>
					<displayCond>FIELD:base.switchableControllerActions:=:TeamSeason->show</displayCond>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>tx_clubms_domain_model_teamseason</allowed>
						<size>1</size>
						<maxItems>1</maxItems>
						<minItems>0</minItems>
						<show_thumbs>1</show_thumbs>
					</config>
				</TCEforms>
			</settings.teamseason.uid>
			<settings.teamseason.showView>
				<TCEforms>
					<label>Gezeigte Einzelansicht</label>
					<displayCond>FIELD:base.switchableControllerActions:=:TeamSeason->show</displayCond>
					<config>
						<type>select</type>
						<renderType>selectSingle</renderType>
						<items type="array">
							<numIndex index="0">
								<numIndex index="0">Index</numIndex>
								<numIndex index="1">index</numIndex>
							</numIndex>
							<numIndex index="2">
								<numIndex index="0">Games</numIndex>
								<numIndex index="1">games</numIndex>
							</numIndex>
							<numIndex index="3">
								<numIndex index="0">Goals</numIndex>
								<numIndex index="1">goals</numIndex>
							</numIndex>
						</items>
					</config>
					<onChange>reload</onChange>
				</TCEforms>
			</settings.teamseason.showView>
		</el>
	</ROOT>
</T3DataStructure>