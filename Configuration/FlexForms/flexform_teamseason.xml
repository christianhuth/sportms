<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<ROOT>
		<TCEforms>
			<sheetTitle>Teaminstanz</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<settings.teamSeason.showViews>
				<TCEforms>
					<label>Erlaube folgende Einzelansichten</label>
					<config>
						<type>select</type>
						<renderType>selectMultipleSideBySide</renderType>
						<items type="array">
							<numIndex index="0">
								<numIndex index="0">Index</numIndex>
								<numIndex index="1">index</numIndex>
							</numIndex>
							<numIndex index="1">
								<numIndex index="0">Competitions</numIndex>
								<numIndex index="1">competitions</numIndex>
							</numIndex>
							<numIndex index="2">
								<numIndex index="0">Games</numIndex>
								<numIndex index="1">games</numIndex>
							</numIndex>
							<numIndex index="3">
								<numIndex index="0">Stats</numIndex>
								<numIndex index="1">stats</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</settings.teamSeason.showViews>
			<settings.teamSeason.showNavigation.enabled>
				<TCEforms>
					<label>Navigation f√ºr Einzelansichten?</label>
					<config>
						<default>1</default>
						<renderType>checkboxToggle</renderType>
						<type>check</type>
					</config>
					<onChange>reload</onChange>
				</TCEforms>
			</settings.teamSeason.showNavigation.enabled>
			<settings.teamSeason.showNavigation.views>
				<TCEforms>
					<label>Zeige folgende Navigationselemente</label>
					<displayCond>
						<AND>
							<numIndex index="1">FIELD:teamSeason.settings.teamSeason.showNavigation.enabled:REQ:TRUE</numIndex>
						</AND>
					</displayCond>
					<config>
						<type>select</type>
						<renderType>selectMultipleSideBySide</renderType>
						<items type="array">
							<numIndex index="0">
								<numIndex index="0">Index</numIndex>
								<numIndex index="1">index</numIndex>
							</numIndex>
							<numIndex index="1">
								<numIndex index="0">Competitions</numIndex>
								<numIndex index="1">competitions</numIndex>
							</numIndex>
							<numIndex index="2">
								<numIndex index="0">Games</numIndex>
								<numIndex index="1">games</numIndex>
							</numIndex>
							<numIndex index="3">
								<numIndex index="0">Stats</numIndex>
								<numIndex index="1">stats</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</settings.teamSeason.showNavigation.views>
		</el>
	</ROOT>
</T3DataStructure>