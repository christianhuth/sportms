							<numIndex index="4">FIELD:base.switchableControllerActions:=:Section->list</numIndex>
							<numIndex index="5">FIELD:base.switchableControllerActions:=:Team->list</numIndex>
							<numIndex index="6">FIELD:base.switchableControllerActions:=:TeamSeason->list</numIndex>
						</OR>
					</displayCond>
					<config>
						<default>1</default>
						<renderType>checkboxToggle</renderType>
						<type>check</type>
					</config>
				</TCEforms>
			</settings.sectionAgeGroup.sectionAgeGroupsSelectbox>
