<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <base>EXT:sportms/Configuration/FlexForms/flexform_base.xml</base>
        <game>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:sportms/Resources/Private/Language/locallang.xlf:tx_sportms_domain_model_game
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.game.uid>
                        <TCEforms>
                            <label>Zeige folgendes Spiel</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_sportms_domain_model_game</allowed>
                                <size>1</size>
                                <maxItems>1</maxItems>
                                <minItems>0</minItems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.game.uid>
                    <settings.game.navigation.enabled>
                        <TCEforms>
                            <label>Spiel-Navigation anzeigen?</label>
                            <config>
                                <default>1</default>
                                <renderType>checkboxToggle</renderType>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.game.navigation.enabled>
                </el>
            </ROOT>
        </game>
    </sheets>
</T3DataStructure>