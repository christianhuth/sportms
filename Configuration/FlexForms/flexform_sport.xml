<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<ROOT>
		<TCEforms>
			<sheetTitle>Sportart</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<settings.sport.uid>
				<TCEforms>
					<label>Zeige folgende Sportart</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>tx_sportms_domain_model_sport</allowed>
						<size>1</size>
						<maxItems>1</maxItems>
						<minItems>0</minItems>
						<show_thumbs>1</show_thumbs>
					</config>
				</TCEforms>
			</settings.sport.uid>
			<settings.sport.sports>
				<TCEforms>
					<label>Welche Sportarten sollen angezeigt werden?</label>
					<config>
						<foreign_table>tx_sportms_domain_model_sport</foreign_table>
						<foreign_table_where>ORDER BY label ASC</foreign_table_where>
						<renderType>selectMultipleSideBySide</renderType>
						<type>select</type>
					</config>
				</TCEforms>
			</settings.sport.sports>
			<settings.sport.sportsSelectbox>
				<TCEforms>
					<label>Selectbox für Sportarten?</label>
					<config>
						<default>1</default>
						<renderType>checkboxToggle</renderType>
						<type>check</type>
					</config>
				</TCEforms>
			</settings.sport.sportsSelectbox>
			<settings.sportAgeGroup.sportAgeGroups>
				<TCEforms>
					<label>Welche Altersgruppen sollen angezeigt werden?</label>
					<config>
						<foreign_table>tx_sportms_domain_model_sportagegroup</foreign_table>
						<foreign_table_where>ORDER BY label ASC</foreign_table_where>
						<renderType>selectMultipleSideBySide</renderType>
						<type>select</type>
					</config>
				</TCEforms>
			</settings.sportAgeGroup.sportAgeGroups>
			<settings.sportAgeGroup.sportAgeGroupsSelectbox>
				<TCEforms>
					<label>Selectbox für Altersgruppen?</label>
					<config>
						<default>1</default>
						<renderType>checkboxToggle</renderType>
						<type>check</type>
					</config>
				</TCEforms>
			</settings.sportAgeGroup.sportAgeGroupsSelectbox>
			<settings.sportAgeLevel.sportAgeLevels>
				<TCEforms>
					<label>Welche Alterstufen sollen angezeigt werden?</label>
					<config>
						<foreign_table>tx_sportms_domain_model_sportagelevel</foreign_table>
						<foreign_table_where>ORDER BY label ASC</foreign_table_where>
						<renderType>selectMultipleSideBySide</renderType>
						<type>select</type>
					</config>
				</TCEforms>
			</settings.sportAgeLevel.sportAgeLevels>
			<settings.sportAgeLevel.sportAgeLevelsSelectbox>
				<TCEforms>
					<label>Selectbox für Alterstufen?</label>
					<config>
						<default>1</default>
						<renderType>checkboxToggle</renderType>
						<type>check</type>
					</config>
				</TCEforms>
			</settings.sportAgeLevel.sportAgeLevelsSelectbox>
		</el>
	</ROOT>
</T3DataStructure>